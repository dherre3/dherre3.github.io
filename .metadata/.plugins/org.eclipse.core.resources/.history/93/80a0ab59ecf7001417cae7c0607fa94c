<script type="text/javascript">
function MyController($scope) { $scope.clock = new Date(); var updateClock = function() {
	$scope.clock = new Date(); };
setInterval(function() { $scope.$apply(updateClock);
}, 1000);
      updateClock();
    $scope.person={
    	name:"David"
    };
  };
